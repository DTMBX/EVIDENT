<script>
(function() {
  const banner = document.getElementById('cookie-consent-banner');
  const acceptBtn = document.getElementById('cookie-consent-accept');
  const rejectBtn = document.getElementById('cookie-consent-reject');
  const customizeBtn = document.getElementById('cookie-consent-customize');
  const modal = document.getElementById('cookie-preferences-modal');
  const cancelBtn = document.getElementById('cookie-prefs-cancel');
  const saveBtn = document.getElementById('cookie-prefs-save');
  
  const CONSENT_KEY = 'cookie_consent';
  const CONSENT_EXPIRY_DAYS = 365;

  function showBanner() {
    banner.style.display = 'block';
    setTimeout(() => {
      banner.style.transform = 'translateY(0)';
      banner.setAttribute('aria-hidden', 'false');
    }, 100);
  }

  function hideBanner() {
    banner.style.transform = 'translateY(100%)';
    banner.setAttribute('aria-hidden', 'true');
    setTimeout(() => {
      banner.style.display = 'none';
    }, 300);
  }

  function showModal() {
    modal.style.display = 'flex';
    document.body.style.overflow = 'hidden';
    // Focus trap
    modal.querySelector('button').focus();
  }

  function hideModal() {
    modal.style.display = 'none';
    document.body.style.overflow = '';
  }

  function setConsent(value, preferences = {}) {
    const expiry = new Date();
    expiry.setDate(expiry.getDate() + CONSENT_EXPIRY_DAYS);
    
    const consentData = {
      value: value,
      preferences: preferences,
      timestamp: new Date().toISOString(),
      expiry: expiry.toISOString()
    };
    
    localStorage.setItem(CONSENT_KEY, JSON.stringify(consentData));
    
    // Update gtag consent if GA is present
    if (typeof gtag === 'function') {
      if (value === 'accepted') {
        gtag('consent', 'update', {
          'analytics_storage': preferences.analytics !== false ? 'granted' : 'denied',
          'ad_storage': preferences.marketing === true ? 'granted' : 'denied'
        });
      } else {
        gtag('consent', 'update', {
          'analytics_storage': 'denied',
          'ad_storage': 'denied'
        });
      }
    }
    
    // Dispatch custom event for other scripts to listen to
    window.dispatchEvent(new CustomEvent('cookieConsentUpdated', { detail: consentData }));
    
    hideBanner();
    hideModal();
  }

  function checkConsent() {
    const consent = localStorage.getItem(CONSENT_KEY);
    if (!consent) {
      showBanner();
      return;
    }
    
    try {
      const consentData = JSON.parse(consent);
      const expiry = new Date(consentData.expiry);
      if (expiry < new Date()) {
        localStorage.removeItem(CONSENT_KEY);
        showBanner();
      }
    } catch (e) {
      localStorage.removeItem(CONSENT_KEY);
      showBanner();
    }
  }

  // Event Listeners
  acceptBtn.addEventListener('click', () => setConsent('accepted', { analytics: true, marketing: true }));
  rejectBtn.addEventListener('click', () => setConsent('rejected', { analytics: false, marketing: false }));
  
  customizeBtn.addEventListener('click', () => {
    hideBanner();
    showModal();
  });

  cancelBtn.addEventListener('click', hideModal);
  
  saveBtn.addEventListener('click', () => {
    const analytics = document.getElementById('cookie-analytics').checked;
    const marketing = document.getElementById('cookie-marketing').checked;
    setConsent('customized', { analytics, marketing });
  });

  // Close modal on backdrop click
  modal.addEventListener('click', (e) => {
    if (e.target === modal) hideModal();
  });

  // Keyboard navigation
  document.addEventListener('keydown', (e) => {
    if (e.key === 'Escape' && modal.style.display === 'flex') {
      hideModal();
    }
  });

  // Check consent on load
  checkConsent();
})();
</script>

