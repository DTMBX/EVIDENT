<script>
// Spotify Player Controller
(function() {
  'use strict';
  
  const container = document.getElementById('spotify-player-container');
  const toggleBtn = document.getElementById('spotify-toggle');
  const closeBtn = document.getElementById('spotify-close');
  const content = document.getElementById('spotify-player-content');
  
  // Load saved state from localStorage
  const savedState = localStorage.getItem('spotifyPlayerExpanded');
  if (savedState === 'true') {
    container.classList.add('spotify-player-expanded');
  }
  
  // Toggle player
  function togglePlayer() {
    container.classList.toggle('spotify-player-expanded');
    const isExpanded = container.classList.contains('spotify-player-expanded');
    localStorage.setItem('spotifyPlayerExpanded', isExpanded);
    
    // Update ARIA
    toggleBtn.setAttribute('aria-expanded', isExpanded);
  }
  
  // Close player
  function closePlayer() {
    container.classList.remove('spotify-player-expanded');
    localStorage.setItem('spotifyPlayerExpanded', 'false');
    toggleBtn.setAttribute('aria-expanded', 'false');
  }
  
  // Event listeners
  toggleBtn.addEventListener('click', togglePlayer);
  closeBtn.addEventListener('click', closePlayer);
  
  // Close on escape key
  document.addEventListener('keydown', function(e) {
    if (e.key === 'Escape' && container.classList.contains('spotify-player-expanded')) {
      closePlayer();
    }
  });
  
  // Keep player state across page navigation (for SPAs)
  window.addEventListener('beforeunload', function() {
    const isExpanded = container.classList.contains('spotify-player-expanded');
    localStorage.setItem('spotifyPlayerExpanded', isExpanded);
  });
  
  console.log('✓ Faith Frontier Spotify Player initialized');
})();
</script>

